<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.menglang.ssh.pojo" auto-import="true">
		<class name="DataDic" table="data_dic">
			<id name="id" column="id">
				<generator class="native"></generator>
			</id>
			<property name="dataDicName" column="data_dic_name"></property>
			<property name="dataDicValue" column="data_dic_value" type="java.lang.String"></property>
		</class>
</hibernate-mapping>
