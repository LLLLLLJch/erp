<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.menglang.ssh.pojo" auto-import="true">
		<class name="CustomerLinkMan" table="customer_linkman">
			<id name="id" column="id">
				<generator class="native"></generator>
			</id>
			<property name="customerId" column="customer_id"></property>
			<property name="linkName" column="link_name" type="java.lang.String"></property>
			<property name="phone" column="phone"></property>
			<property name="position" column="position"></property>
			<property name="gender" column="gender"></property>
		</class>
</hibernate-mapping>
